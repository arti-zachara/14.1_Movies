{
  "name": "14.1",
  "version": "1.0.0",
  "description": "",
  "main": "index.html",
  "scripts": {
    "test": "npm run test:html && npm run test:js",
    "test:html": "globstar nu-html-checker templates/*.html",
    "test:js": "jshint",
    "init-project": "npm install && npm-run-all init:*",
    "init:dirs": "mkdirp sass css vendor images js",
    "init:files": "touch README.md index.html sass/style.scss js/script.js",
    "init:gitignore": "curl https://raw.githubusercontent.com/github/gitignore/master/Node.gitignore -o .gitignore",
    "start": "nodemon index.js",
    "build": "npm-run-all build:* test",
    "build:clean": "rimraf css/*.css",
    "build:sass": "node-sass --output-style compact -o css sass",
    "build:autoprefixer": "autoprefixer-cli css/style.css",
    "build-dev:sass": "node-sass --output-style expanded --source-map true -o css sass",
    "watch": "npm-run-all build -p watch:*",
    "watch:sassprefixer": "onchange -d 100 sass/style.scss -- npm-run-all -p sassprefixer:*",
    "watch:browsersync": "browser-sync start --server --files css/*.css templates/*.html",
    "sassprefixer:sass": "node-sass --output-style expanded --source-map true -o css sass",
    "sassprefixer:autoprefixer": "autoprefixer-cli -o css/style.prefixed.css css/style.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arti-zachara/14.1_Movies.git"
  },
  "keywords": [
    "react",
    "movies",
    "key_use"
  ],
  "author": "arti Zachara",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/arti-zachara/14.1_Movies/issues"
  },
  "homepage": "https://github.com/arti-zachara/14.1_Movies#readme",
  "devDependencies": {
    "autoprefixer-cli": "^1.0.0",
    "browser-sync": "^2.26.3",
    "globstar": "^1.0.0",
    "html-validator-cli": "^5.0.0",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "nu-html-checker": "^0.1.0",
    "onchange": "^5.2.0",
    "rimraf": "^2.6.3",
    "sass-graph": "^3.0.4",
    "jshint": "^2.9.5"
  }
}
